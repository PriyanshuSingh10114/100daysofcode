//509. Fibonacci Number

class Solution {
public:
    int fib(int n){
        if(n==0||n==1){
            return n;
        }
        else{
            return fib(n-1)+fib(n-2);
        }
    
    }
};


//2427. Number of Common Factors

class Solution {
public:
    int commonFactors(int a, int b) {
        int count=0;
        for(int i=1;i<=a||i<=b;i++){
            if(a%i==0 && b%i==0){
                count++;
            } 
